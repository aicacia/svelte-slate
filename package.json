{
	"name": "svelte-slate",
	"description": "slate svelte view layer",
	"version": "0.1.24",
	"license": "(MIT OR Apache-2.0)",
	"type": "module",
	"author": "Nathan Faucett",
	"repository": {
		"type": "git",
		"url": "https://github.com/nathanfaucett/svelte-slate"
	},
	"homepage": "https://github.com/nathanfaucett/svelte-slate#readme",
	"keywords": [
		"svelte",
		"slate",
		"editor",
		"text",
		"rich"
	],
	"scripts": {
		"start": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"clean": "rm -rf node_modules/.vite .svelte-kit package",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"publish": "npm run package && cd package && npm publish --access public",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@playwright/test": "1",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@types/is-url": "1",
		"@types/throttle-debounce": "2",
		"@typescript-eslint/eslint-plugin": "5",
		"@typescript-eslint/parser": "5",
		"eslint": "8",
		"eslint-config-prettier": "8",
		"eslint-plugin-svelte3": "3",
		"image-extensions": "1",
		"is-url": "1",
		"lorem-ipsum": "2",
		"prettier": "2",
		"prettier-plugin-svelte": "2",
		"slate": "0",
		"slate-history": "0",
		"svelte": "3",
		"svelte-check": "2",
		"svelte-icons": "2",
		"svelte-portal": "2",
		"svelte-preprocess": "4",
		"svelte2tsx": "0",
		"tslib": "2",
		"typescript": "4"
	},
	"peerDependencies": {
		"slate": "0",
		"svelte": "3",
		"tslib": "2"
	},
	"dependencies": {
		"fast-equals": "3",
		"scroll-into-view-if-needed": "2",
		"throttle-debounce": "3"
	}
}
