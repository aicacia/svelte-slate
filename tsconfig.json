{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"downlevelIteration": true,
		"resolveJsonModule": true,
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"svelte-slate": ["src/lib"],
			"svelte-slate/*": ["src/lib/*"]
		}
	}
}
